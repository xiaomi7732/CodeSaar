{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build di-console",
            "program": "${workspaceFolder}/src/Console/bin/Debug/net5.0/DI.Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Console",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (AnitPatterns WebAPI)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build anit-pattern example",
            "program": "${workspaceFolder}/src/AntiPatterns/bin/Debug/net5.0/AntiPatterns.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/AntiPatterns",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}