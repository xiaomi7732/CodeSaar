    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch UseWithDI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build use-with-di example",
            "program": "${workspaceFolder}/examples/UseWithDI/bin/Debug/net5.0/UseWithDI.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/UseWithDI",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch GetStarted",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build get-started example",
            "program": "${workspaceFolder}/examples/GetStartedConsole/bin/Debug/net5.0/GetStartedConsole.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/GetStartedConsole",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch HostedService",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build generic host: server",
            "program": "${workspaceFolder}/examples/NuGetConsumer/Server.HostedConsole/bin/Debug/net5.0/Server.HostedConsole.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/NuGetConsumer/Server.HostedConsole/",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}